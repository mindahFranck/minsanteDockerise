{
  "name": "health-management-api",
  "version": "1.0.0",
  "description": "Health Management System API with Express, TypeScript, Sequelize & MySQL",
  "keywords": [
    "express",
    "typescript",
    "sequelize",
    "mysql",
    "api"
  ],
  "license": "MIT",
  "author": "",
  "main": "dist/server.js",
  "scripts": {
    "db:migrate": "tsx src/database/migrate.ts",
    "db:seed": "tsx src/database/seeders/index.ts",
    "db:init": "tsx src/scripts/init-db.ts",
    "db:init:force": "FORCE_CONFIRM=YES tsx src/scripts/init-db.ts --force",
    "docker:build": "docker build -t health-management-api .",
    "docker:down": "docker-compose down",
    "docker:up": "docker-compose up -d",
    "lint": "echo 'Linting not configured yet'",
    "test": "jest --coverage",
    "build": "tsc --build --force",
    "dev": "tsx src/server.ts",
    "start": "node dist/server.js",
    "test:watch": "jest --watch"
  },
  "dependencies": {
